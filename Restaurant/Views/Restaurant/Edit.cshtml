@model MealEditViewModel; // <---

<form asp-controller="Restaurant" asp-action="Edit" method="post" enctype="multipart/form-data"> @*<--- Edit*@
	<div class="form-group">
		<label asp-for="Name"></label>
		<input asp-for="Name" class="form-control" />
		<span asp-validation-for="Name" class="text-danger"></span>
	</div>

	<div class="form-group">
		<label asp-for="Price"></label>
		<input asp-for="Price" class="form-control" />
		<span asp-validation-for="Price" class="text-danger"></span>
	</div>

	<div class="form-group">
		<label asp-for="Description"></label>
		<input asp-for="Description" class="form-control" />
		<span asp-validation-for="Description" class="text-danger"></span>
	</div>

	<div class="form-group">
		<label asp-for="Photo"></label>
		<p><img src="@Model.PhotoUrl" /></p>
		<input asp-for="Photo" class="form-control" />
		<span asp-validation-for="Photo" class="text-danger"></span>
	</div>


	<button class="btn btn-primary" type="submit">Edit meal</button> @*<--- Edit*@
</form>